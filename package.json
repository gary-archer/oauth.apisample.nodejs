{
  "name": "sampleapi",
  "version": "3.0.0",
  "description": "A NodeJS version of the Final OAuth secured API",
  "scripts": {
    "postinstall": "./setup.sh",
    "lint": "npx eslint . --ext .ts",
    "build": "ts-node --files src/host/startup/app.ts",
    "start": "nodemon -e ts --exec \"npm run build\"",
    "buildRelease": "rimraf dist && tsc",
    "startRelease": "node dist/host/startup/app.js"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "guid-typescript": "^1.0.9",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "node-cache": "^5.1.2",
    "openid-client": "^4.7.4",
    "reflect-metadata": "^0.1.13",
    "tunnel-agent": "^0.6.0",
    "url": "^0.11.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.2"
  }
}
